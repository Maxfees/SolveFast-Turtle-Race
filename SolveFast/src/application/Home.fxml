<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@finalhome.png" />
         </image>
      </ImageView>
      <Button fx:id="startbutton" layoutX="113.0" layoutY="99.0" mnemonicParsing="false" onAction="#start" prefHeight="64.0" prefWidth="236.0" styleClass="startgame" stylesheets="@application.css" text="Start Game">
         <font>
            <Font name="Times New Roman Bold" size="34.0" />
         </font>
      </Button>
      <Button fx:id="rulesbutton" layoutX="113.0" layoutY="194.0" mnemonicParsing="false" onAction="#insta" prefHeight="64.0" prefWidth="236.0" styleClass="instruction" stylesheets="@application.css" text="Instruction">
         <font>
            <Font name="Times New Roman Bold" size="34.0" />
         </font>
      </Button>
      <Button fx:id="exitbutton" layoutX="113.0" layoutY="289.0" mnemonicParsing="false" onAction="#logout" prefHeight="64.0" prefWidth="236.0" styleClass="exit" stylesheets="@application.css" text="Exit">
         <font>
            <Font name="Times New Roman Bold" size="34.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
